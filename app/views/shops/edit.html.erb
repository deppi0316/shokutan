<h2>店舗情報の編集</h2>

<%= form_for @shop do |f| %>
  <%= render "devise/shared/error_messages", resource: @shop %>

  <div class="field">
    <%= f.label :address %><br />
    <%= f.text_field :address %>
  </div>

  <div class="field">
    <%= f.label :phone_number %><br />
    <%= f.text_field :phone_number %>
  </div>

  <div class="field">
    <%= f.label :station %><br />
    <%= f.text_field :station %>
  </div>

  <div class="field"> 
    <%= f.label :parking_id %><br />
    <%= f.collection_select(:parking_id, Parking.all, :id, :name, {}) %>
  </div>

  <div class="field">
    <%= f.label :year %><br />
    <%= f.text_field :year %>
  </div>

  <div class="field">
    <%= f.label :month %><br />
    <%= f.text_field :month %>
  </div>

  <div class="field">
    <%= f.label :day %><br />
    <%= f.text_field :day %>
  </div>

  <div class="field">
    <%= f.label :url %><br />
    <%= f.text_field :url %>
  </div>

  <div class="actions">
    <%= f.submit "Sign up" %>
    <%=link_to '戻る', root_path%>
  </div>

<% end %>
